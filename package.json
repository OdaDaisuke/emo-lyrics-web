    "aphrodite": "^2.1.0",
    "axios": "^0.18.0",
    "browser-sync": "^2.24.4",
    "browser-sync-webpack-plugin": "^2.2.2",
    "firebase": "^5.5.1",
    "memoizee": "^0.4.14",
    "mobx": "^4.1.0",
    "puppeteer": "^1.2.0",
